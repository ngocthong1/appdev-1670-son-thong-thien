@model AppDev.Models.RequestCategory

@{
    ViewData["Title"] = "Create";
}

<h1 class="text-primary">Create</h1>

<h4 class="text-secondary">RequestCategory</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input asp-for="StoreOwnerId" type="hidden" />
            <div class="form-group">
                <label asp-for="Name" class="control-label"><span class="text-dark"></span></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
